{
  "name": "Webhook on Request",
  "version": "1.0",
  "description": "Trigger a webhook when your browser sends a request to a particular domain.",
  "icons": {
    "16": "icons/icons8-webhook-16.png",
    "48": "icons/icons8-webhook-48.png",
    "96": "icons/icons8-webhook-96.png",
    "128": "icons/icons8-webhook-128.png",
    "144": "icons/icons8-webhook-144.png",
    "240": "icons/icons8-webhook-240.png",
    "480": "icons/icons8-webhook-480.png"
  },
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options/options.html",
  "permissions": [
    "webNavigation",
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {}
}
